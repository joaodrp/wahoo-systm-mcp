{
  "name": "wahoo-systm-mcp",
  "version": "0.1.0",
  "description": "MCP server for Wahoo SYSTM calendar and workout data",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "wahoo-systm-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json,md}\""
  },
  "keywords": [
    "mcp",
    "wahoo",
    "systm",
    "workout",
    "cycling"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "node-fetch": "^3.3.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/ui": "^4.0.15",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "^5.7.2",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
